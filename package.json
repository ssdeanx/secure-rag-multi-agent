  "type": "module",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "dotenvx run -- concurrently -n \"NEXT,MASTRA\" -c \"blue.bold,green.bold\" \"npm run dev:next\" \"npm run dev:mastra\"",
    "dev:next": "next dev --turbopack",
    "dev:mastra": "mastra dev --dir src/mastra",
    "build": "dotenvx run -- next build",
    "start": "next start",
    "mastra-dev": "mastra dev --dir src/mastra",
    "lint": "next lint",
    "pretty": "npx prettier --write .",
    "build-cli": "tsc -p tsconfig.cli.json",
    "cli": "node dist/index.js",
    "generate-index": "node dist/cli/index.js",
    "validate:agents": "node scripts/validate-agents-docs.mjs",
    "sync:agents": "node scripts/sync-agents-docs.mjs",
    "sync:agents:write": "node scripts/sync-agents-docs.mjs --write",
    "check:agents:sync": "node scripts/check-agents-sync.mjs"
  },
  "keywords": [
    "rag",
    "mastra",
    "security",
    "governance",
    "ai"
